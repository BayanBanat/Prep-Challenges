            "source": [
                "function howManyGames(p, d, m, s) {\n    // Return the number of games you can buy\n    let numberOfBuying=0;\n    let currentPrice=p;\n    while(s>=currentPrice){\n        numberOfBuying++;\n        s-=currentPrice;\n        currentPrice=Math.max(currentPrice-d,m);\n    }\n    return numberOfBuying;\n  \n  }"
