        },
        {
            "language": "javascript",
            "source": [
                "function equalizeArray(arr) {\n    // Write your code here\n  let count={};\n  let maxCount=0;\n  for(let i=0;i<arr.length;i++){\n     if (!count[arr[i]]){\n         count[arr[i]]=0;\n     }\n     count[arr[i]]++;\n     maxCount=Math.max(maxCount,count[arr[i]]);\n  }\n  return arr.length-maxCount;\n  }"
            ],
