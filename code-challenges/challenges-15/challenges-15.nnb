            "source": [
                "function squares(a, b) {\n    // Write your code here\n  let count=0;\n  for(let i=a; i<=b;i++){\n    let square=Math.sqrt(i);\n    if(square===Math.floor(square)){\n        count++;\n    }\n  }\n  return count;\n  }"
